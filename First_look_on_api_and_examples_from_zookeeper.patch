+    session.create_semaphore("my-semaphore", 10, "my-data")
+
+# синхронно
+with session.semaphore("my-semaphore"):
+    # критическая секция
+    do_work()
+
+# асинхронно
+async with session.semaphore_async("my-semaphore"):
+    await do_work_async()
+
+def on_semaphore_changed(changed):
+    if changed:
+        print("Semaphore state changed!")
+    else:
+        print("Watch expired, need to resubscribe")
+
+settings = DescribeSemaphoreSettings(
+    on_changed=on_semaphore_changed,
+    watch_data=True,
+    watch_owners=True,
+    include_owners=True,
+    include_waiters=True
+)
+
+desc = session.describe_semaphore("my-semaphore", settings=settings)
+
+print(f"Semaphore {desc.name}: {desc.count}/{desc.limit}")
+for owner in desc.owners:
+    print(f"Owner {owner.session_id} with {owner.count} tokens")
