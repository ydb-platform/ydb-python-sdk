+class CoordinationClient:
+    def __init__(self, driver):
+        self.driver = driver
+        # driver должен быть инициализирован и готов к работе
+
+    # ------------------ Создание узла ------------------
+    def create(self, path: str, ttl: int = 60):
+        pass
+
+    def lock(self):
+        # ---Создание сессии и так далее ( а тут в локе инкапсулирована вся логика)------------------
+        return CoordinationLock()
+
+
+    # ------------------ Получение содержимого узла ------------------
+    def describe(self, path: str):
+        pass
+
+    # ------------------ Удаление узла ------------------
+    def delete(self, path: str):
+        pass
+
+
+class CoordinationLock:
+    def enter , exit - context manager
+
+    # ------------------ Захват узла ------------------
+    def acquire(self, path: str, timeout: Optional[int] = None):
+        pass
+
+
+    # ------------------ Освобождение узла ------------------
+    def release(self, path: str):
+        pass
